@model IEnumerable<CourseRegistrationProgram.Models.InstructorsModel>

@{
    ViewData["Title"] = "Instructors List";
}


<h2>Instructors: </h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Course</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.EmailAddress</td>
            <td>@item.Course</td>
        </tr>
        }
    </tbody>





</table>


